<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    
    <navigation-rule>  
        <navigation-case>  
            <from-outcome>home</from-outcome>  
            <to-view-id>/home.xhtml</to-view-id>  
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>visualizzaLocale</from-outcome>
            <to-view-id>/visualizzaLocale.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>locali</from-outcome>
            <to-view-id>/locali.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>localiUtente</from-outcome>
            <to-view-id>/localiUtente.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>newsBean</managed-bean-name>
        <managed-bean-class>jsfpkg.NewsBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
